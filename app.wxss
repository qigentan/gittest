.modal {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
  .modal-mask {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    background-color: rgba(0, 0, 0, .5);
  }
  .modal-content {
    position: absolute;
    width: 360rpx;
    height: 200rpx;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    background-color: #fff;
    text-align: center;
    color: #666;
    font-size: 30rpx;
    border-radius: 6rpx;
    padding-bottom: 20rpx;
  }
  .loading-dot {
    margin: 60rpx auto;
    margin-bottom: 40rpx;
    display: flex;
    justify-content: center;
    .dot1,
    .dot2,
    .dot3 {
      width: 14rpx;
      height: 14rpx;
      border-radius: 100%;
      background-color: rgba(0, 0, 0, .5);
      margin: 0 10rpx;
      animation: dot-animate .9s linear infinite normal;
    }
    .dot2 {
      background-color: rgba(0, 0, 0, .3);
      animation-delay: .3s;
    }
    .dot3 {
      background-color: rgba(0, 0, 0, .1);
      animation-delay: .6s;
    }
  }
}

@keyframes dot-animate {
  0%,
  100% {
    background-color: rgba(0, 0, 0, .5);
  }
  50% {
    background-color: rgba(0, 0, 0, .1);
  }
}


@keyframes loading-hu {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}

.loading-hu {
  width: 100%;
