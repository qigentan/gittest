/*数字键盘*/

.maskBg {
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 99;
}

.keyboard {
  background: #fff;
  z-index: 49999;
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  height: 0;
  overflow: hidden;
  transform: translateY(100%);
}

.keyboard .header {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
}

.keyboard .header .cancel,
.keyboard .header .confirm {
  float: left;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 30rpx;
}

.keyboard .header .confirm {
  float: right;
  color: rgba(243, 124, 62, 1);
}

.keyboard .header::after,
.keyboard .number::after {
  content: '';
  display: table;
  clear: both;
}

.keyboard .number .item {
  width: 33.333%;
  float: left;
  color: rgba(102, 102, 102, 1);
  font-size: 36rpx;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  border-top: 1px solid #d9d9d9;
  border-right: 1px solid #d9d9d9;
  box-sizing: border-box;
}

.keyboard .number .item:active {
  border: 1px solid rgba(245, 123, 51, 1);
  color: rgba(245, 123, 51, 1);
}

.keyboard .number .item:nth-child(3n) {
  border-right: 0;
}

.keyboard .number .item.selected {
  border: 1px solid rgba(245, 123, 51, 1);
  color: rgba(245, 123, 51, 1);
}

.keyboard .number .item.none {
  background: rgba(248, 248, 248, 1);
}

.keyboard .number .item.none:active {
  color: rgba(102, 102, 102, 1);
  border-top: 1px solid #d9d9d9;
  border-right: 1px solid #d9d9d9;
  border-left: 0;
  border-bottom: 0;
}

.keyboard .number .item.delete {
  background: url('https://res.tuhu.org/static/wx/images/keeplist2/delete.png') no-repeat center rgba(248, 248, 248, 1);
  background-size: 55rpx 35rpx;
}


/*纯文字提示*/

.toast-bg {
  position: fixed;
  top: 0;
  bottom: 0;
  z-index: 999999;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .2);
}


/*水平居中必备样式*/

.toast-center {
  position: fixed;
  z-index: 9999999;
  width: 100%;
  height: 100%;
  text-align: center;
}

.toast {
  display: inline-block;
  padding: 20rpx 40rpx;
  max-width: 600rpx;
  font-size: 28rpx;
  color: #fff;
  background: rgba(0, 0, 0, .5);
  border-radius: 10rpx;
  text-align: center;
}


/*垂直居中必备样式*/

.toast-center::after {
  content: '';
  display: inline-block;
  width: 0;
  height: 100%;
  vertical-align: middle;
}

.toast .toast-icon {
  display: block;
  margin: 0 auto 10rpx auto;
  width: 50rpx;
  height: 50rpx;
}


